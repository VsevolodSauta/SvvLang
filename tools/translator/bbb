Object List_Search(Object self, Object object)
{
	Object iterator;
	Object self.iterator;
	iterator = self.iterator;
	Object_ToBegin(iterator);
	Object list;
	list = List_Create();
	Object_Autorelease(list);
	Object_SearchFirst(iterator, object);
	while
	{
		Object_PushBack(list, Object_TempClone(iterator));
		Object_SearchFirst(iterator, object);
	}
	return
}


Object List_Size(Object self)
{
	Object_ToBegin(self.iterator);
	Object toReturn;
	Object 0;
	toReturn = 0;
	while
	{
		Object_Inc(toReturn);
		Object_next(self.iterator);
	}
	Object_Hide(self.iterator);
	return
}


Object List_Concatenate(Object self, Object list)
{
	Object_ToEnd(self.iterator);
	Object_AddListAfter(self.iterator, list);
	Object_Hide(self.iterator);
	return
}


Object List_SublistBetweenIterators(Object self, Object positionFrom, Object positionTo)
{
	iterator = Object_Clone(positionFrom);
	list = List_Create();
	while
	{
		Object_PushBack(list, iterator.node.data);
		Object_Next(iterator);
	}
	Object_Release(iterator);
	return
}

